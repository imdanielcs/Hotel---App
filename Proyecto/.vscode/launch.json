{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Ionic Serve",
        "url": "http://localhost:8100",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "sourceMapPathOverrides": {
          "webpack:///./src/*": "${webRoot}/*"
        }
      }
    ]
  }
  